/* Write a function that takes three string arguments (first, last, word) and returns true 
if (when alphabetically sorted) word is found between first and last. */

public class Program
{
    public static bool isBetween(string first, string last, string word)
    {
	string[] check = new string[] {first, last, word};
	System.Array.Sort(check);
			
	return check[1] == word;
    }
}
